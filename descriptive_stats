    "# Median"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "e6810297",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "46850.0\n"
     ]
    }
   ],
   "source": [
    "#formula\n",
    "n = len(data['Income'])\n",
    "if n % 2:\n",
    "    median_income = sorted(data['Income'])[round(0.5*(n-1))]\n",
    "else:\n",
    "    x_ord, index = sorted(data['Income']), round(0.5 * n)\n",
    "    median_income = 0.5 * (x_ord[index-1] + x_ord[index])\n",
    "\n",
    "print(median_income)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
